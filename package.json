    "nestjs-pino": "^4.2.0",
    "node-tesseract-ocr": "^2.2.0",
    "nodemailer": "^6.9.16",
    "officeparser": "^5.2.1",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.0.0",

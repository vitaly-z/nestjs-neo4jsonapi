    "pino-pretty": "^13.0.0",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.1",
    "stripe": "^20.0.0",
    "tslib": "^2.8.1",
    "turndown": "^7.2.2",
    "web-push": "^3.6.7",
    "xlsx-republish": "^0.20.3",
    "zod": "^4.0.0",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@nestjs/bullmq": "^11.0.4",
    "@swc/core": "^1.15.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/qs": "^6.9.17",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "jsonapi",
    "neo4j",
    "redis",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlonicora/nestjs-neo4jsonapi"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",

{
  "name": "@carlonicora/nestjs-jsonapi",
  "version": "0.0.1",
  "description": "NestJS foundation package with JSON:API, Neo4j, Redis, and common utilities",
  "author": "Carlo Nicora",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.mjs",
      "require": "./dist/common/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.mjs",
      "require": "./dist/core/*.js"
    },
    "./foundations": {
      "types": "./dist/foundations/index.d.ts",
      "import": "./dist/foundations/index.mjs",
      "require": "./dist/foundations/index.js"
    },
    "./foundations/*": {
      "types": "./dist/foundations/*.d.ts",
      "import": "./dist/foundations/*.mjs",
      "require": "./dist/foundations/*.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.mjs",
      "require": "./dist/agents/index.js"
    },
    "./bootstrap": {
      "types": "./dist/bootstrap/index.d.ts",
      "import": "./dist/bootstrap/index.mjs",
      "require": "./dist/bootstrap/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/throttler": "^6.0.0",
    "fastify": "^5.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/jwt": {
      "optional": true
    },
    "@nestjs/passport": {
      "optional": true
    },
    "@nestjs/throttler": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "passport": {
      "optional": true
    },
    "passport-jwt": {
      "optional": true
    },
    "@nestjs/bullmq": {
      "optional": true
    },
    "@nestjs/websockets": {
      "optional": true
    },
    "@nestjs/platform-socket.io": {
      "optional": true
    },
    "@nestjs/schedule": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@nestjs/passport": "^11.0.0",
    "@nestjs/throttler": "^6.0.0",
    "fastify": "^5.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "neo4j-driver": "^5.27.0",
    "ioredis": "^5.4.2",
    "stripe": "^17.5.0",
    "@langchain/core": "^0.3.26",
    "@langchain/openai": "^0.3.16",
    "@langchain/langgraph": "^0.2.38",
    "bullmq": "^5.34.8",
    "@sendgrid/mail": "^8.1.4",
    "nodemailer": "^6.9.16",
    "@aws-sdk/client-s3": "^3.712.0",
    "@aws-sdk/s3-request-presigner": "^3.712.0",
    "@azure/storage-blob": "^12.26.0",
    "socket.io": "^4.8.1",
    "pino": "^9.6.0",
    "nestjs-pino": "^4.2.0",
    "pino-pretty": "^13.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "web-push": "^3.6.7",
    "nestjs-cls": "^4.5.0",
    "handlebars": "^4.7.8",
    "bcryptjs": "^2.4.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/passport-jwt": "^4.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "jsonapi",
    "neo4j",
    "redis",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlonicora/nestjs-jsonapi"
  },
  "publishConfig": {
    "access": "public"
  }
}

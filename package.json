{
  "name": "@carlonicora/nestjs-neo4jsonapi",
  "version": "1.41.0",
  "description": "NestJS foundation package with JSON:API, Neo4j, Redis, LangChain agents, and common utilities",
  "author": "Carlo Nicora",
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "neo4jsonapi-migrate": "./dist/tools/migrate-entity/index.js",
    "generate-module": "./dist/scripts/generate-module/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "default": "./dist/common/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "default": "./dist/core/*.js"
    },
    "./foundations": {
      "types": "./dist/foundations/index.d.ts",
      "default": "./dist/foundations/index.js"
    },
    "./foundations/*": {
      "types": "./dist/foundations/*.d.ts",
      "default": "./dist/foundations/*.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "default": "./dist/agents/index.js"
    },
    "./bootstrap": {
      "types": "./dist/bootstrap/index.d.ts",
      "default": "./dist/bootstrap/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tools/migrate-entity/tsconfig.json && tsc -p scripts/generate-module/tsconfig.json && cp -R src/core/email/templates dist/core/email/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\" \"tools/**/*.spec.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:stripe": "vitest run --testPathPattern=stripe",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@azure/storage-blob": "^12.30.0",
    "@getbrevo/brevo": "^3.0.1",
    "@langchain/community": "^1.1.7",
    "@langchain/core": "^1.1.17",
    "@langchain/google-vertexai": "^2.1.13",
    "@langchain/langgraph": "^1.1.2",
    "@langchain/openai": "^1.2.3",
    "@langchain/textsplitters": "^1.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/instrumentation-http": "^0.211.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.57.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@sendgrid/mail": "^8.1.6",
    "@simplewebauthn/server": "^13.2.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.67.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "discord.js": "^14.25.1",
    "fast-xml-parser": "^5.3.3",
    "fastify": "^5.7.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.23",
    "marked": "^17.0.1",
    "mathjs": "^15.1.0",
    "natural": "^8.1.0",
    "necord": "^6.12.0",
    "neo4j-driver": "^6.0.1",
    "nestjs-pino": "^4.5.0",
    "nestjs-redoc": "^2.2.2",
    "node-tesseract-ocr": "^2.2.1",
    "nodemailer": "^7.0.12",
    "officeparser": "^6.0.4",
    "openai": "^6.16.0",
    "otpauth": "^9.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.530",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "qrcode": "^1.5.4",
    "qs": "^6.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "stripe": "^20.2.0",
    "tslib": "^2.8.1",
    "turndown": "^7.2.2",
    "web-push": "^3.6.7",
    "xlsx-republish": "^0.20.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/testing": "^11.1.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.15.10",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.0.10",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/qs": "^6.14.0",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.18",
    "commander": "^14.0.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "glob": "^13.0.0",
    "globals": "^17.1.0",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "jsonapi",
    "neo4j",
    "redis",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlonicora/nestjs-neo4jsonapi"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@fastify/multipart": "^9.0.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "nestjs-cls": "^6.2.0",
    "nestjs-i18n": "^10.6.0",
    "zod": "^4.1.13"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  }
}
